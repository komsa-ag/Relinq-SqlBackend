<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <ProjectType>Library</ProjectType>
  </PropertyGroup>

  <Import Project="..\Shared.build.props" />

  <PropertyGroup>
    <AssemblyTitle>re-linq SQL Backend</AssemblyTitle>
    <Description>re-linq SQL Backend: A framework for generating SQL based on the re-linq Frontend.</Description>
    <AssemblyName>Remotion.Linq.SqlBackend</AssemblyName>
